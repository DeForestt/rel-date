.needs <io>
.needs <std>
.needs "head/CLineArgs"
.needs "head/DateFunctions"

int testDate(){
	assert(daysSinceJan1(1,1,2000) == 0, "daysSinceJan1(1,1,2000) == 0");
	return 0;
};

int testdateRelativeTo(){
	assert(dateRelativeTo(1,1,2000,2000) == 0, "dateRelativeTo(1,1,2000,2000) == 1");
	assert(dateRelativeTo(1,1,2000,1999) == 365, "dateRelativeTo(1,1,2000,1999) == 365");
};

int main(int argc, adr argv){
	testDate();
	testdateRelativeTo();
	assert(1 == 1, "Failed 1 == 1 assert");
	return 0;
};
